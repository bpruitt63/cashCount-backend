{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/loginSchema.json",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "password": {
            "type": "string",
            "minLength": 6,
            "maxLength": 20
        }
    },
    "additionalProperties": false,
    "required": [
      "id",
      "password"
    ]
}