{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/countNew.json",
    "type": "object",
    "properties": {
        "userId": {
            "type": "string"
        },
        "cash": {
            "type": "number"
          },
        "time": {
            "type": "string"
        },
        "timestamp": {
            "type": "number"
        },
        "note": {
            "oneOf": [
                { "type": "string" },
                { "type": "null" }
            ]  
        }
    },
    "additionalProperties": false,
    "required": [
        "userId",
        "cash",
        "time",
        "timestamp",
        "note"
    ]
}